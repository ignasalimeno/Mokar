      <script type="text/javascript">
        var itemList = [];

        function checkAll(itemId) {
                      //document.getElementById(itemId).checked = true;
            var ck = document.getElementById("ckTodas");
            
            if (ck.checked == true) {
                var cks = document.getElementsByName("ck");
                for (i = 0; i < cks.length; i++) {
                    cks[i].checked = true;
                                    }
            } else {
                var cks = document.getElementsByName("ck");
                for (i = 0; i < cks.length; i++) {
                    cks[i].checked = false;
                }
            }

            cargarChequeados();
        }

        function DesCheck(itemId) {
            var itemList = [];
            var ck = document.getElementById(itemId);
            if (ck.type == "checkbox") {
                if (ck.checked == false) {
                    var ckT = document.getElementById("ckTodas");
                    ckT.checked = false;
                } 
            }

          cargarChequeados();
        }

        function cargarChequeados() {
            var itemList = [];

            var cks = document.getElementsByName("ck");

                for (i = 0; i < cks.length; i++) {
                    if (cks[i].checked == true) {
                        itemList.push(cks[i].id);
                    }
                }

            document.getElementById('<%= HiddenField1.ClientID %>').value = itemList;
        }

    </script>

    <script type="text/javascript">
        function EnterKeyFilter() {
            if (window.event.keyCode == 13) {
                event.returnValue = false;
                event.cancel = true;
            }
        }
    </script>

