                                    <%--Contenido del Modal--%>

                                    <%-- Comienza el Registro --%>
                                    <p>Si aún no estas registrado, hacelo!</p>
                                    <div class="Form-group2" runat="server">

                                        <asp:Label ID="lblSeleccionarTipo" runat="server" Text="Seleccione el tipo de cliente"></asp:Label>
                                        <asp:UpdatePanel ID="UpdatePanel1" runat="server">
                                            <ContentTemplate>
                                                <asp:DropDownList CssClass="form-control" ID="DDL_Roles" runat="server" AutoPostBack="true" OnSelectedIndexChanged="cambioTipoDeCliente"></asp:DropDownList>
                                                <br />
                                                <div class="Form-group2" runat="server">
                                                    <asp:Label ID="lblNombre" runat="server" Text="Ingresar Nombre y Apellido"></asp:Label>
                                                    <asp:TextBox CssClass="form-control" ID="TB_Nombre" runat="server" MaxLength="50" Width="100%"></asp:TextBox>
                                                    <asp:RequiredFieldValidator ID="RFV_TBNombre" runat="server" ErrorMessage="Este campo es requerido" ControlToValidate="TB_Nombre"
                                                        ForeColor="Red" Font-Bold="true" ValidationGroup="Registrarse"></asp:RequiredFieldValidator>
                                                </div>

                                                <div class="Form-group2">
                                                    <asp:Label ID="lblDNI" runat="server" Text="Ingresar el DNI"></asp:Label>
                                                    <asp:TextBox CssClass="form-control" ID="tb_Dni" runat="server" Maxlenght="50" Width="100%"></asp:TextBox>
                                                    <asp:RequiredFieldValidator ID="RFV_tbDni" runat="server" ErrorMessage="Este campo es requerido" ControlToValidate="tb_Dni"
                                                        ForeColor="Red" Font-Bold="true" ValidationGroup="Registrarse"></asp:RequiredFieldValidator>
                                                </div>

                                            </ContentTemplate>
                                            <Triggers>
                                                <asp:AsyncPostBackTrigger ControlID="DDL_Roles" EventName="SelectedIndexChanged" />
                                            </Triggers>

                                        </asp:UpdatePanel>
                                    </div>

                                    <div class="Form-group2">
                                        <label>Ingresar la dirección</label>
                                        <asp:TextBox CssClass="form-control" ID="txtDireccion" runat="server" MaxLength="150" Width="100%"></asp:TextBox>
                                        <asp:RequiredFieldValidator ID="RFV_Direccion" runat="server" ErrorMessage="Este campo es requerido" ControlToValidate="txtDireccion"
                                            ForeColor="Red" Font-Bold="true" ValidationGroup="Registrarse"></asp:RequiredFieldValidator>
                                    </div>

                                    <div class="Form-group2">
                                        <label>Ingresar el telefono</label>
                                        <asp:TextBox CssClass="form-control" ID="txtTel" runat="server" MaxLength="50" Width="100%"></asp:TextBox>
                                        <asp:RequiredFieldValidator ID="RFV_Telefono" runat="server" ErrorMessage="Este campo es requerido" ControlToValidate="txtTel"
                                            ForeColor="Red" Font-Bold="true" ValidationGroup="Registrarse"></asp:RequiredFieldValidator>
                                    </div>

                                    <div class="Form-group2">
                                        <label>Seleccione el tipo de Usuario</label>
                                        <asp:DropDownList CssClass="form-control" ID="DDL_Planes" runat="server"
                                            AutoPostBack="true">
                                        </asp:DropDownList>
                                    </div>
                                    <br />

                                    <div class="Form-group2">
                                        <label>Ingresar el email</label>
                                        <asp:TextBox CssClass="form-control" ID="TB_email" runat="server" TextMode="Email" MaxLength="50" Width="100%"></asp:TextBox>
                                        <asp:RequiredFieldValidator ID="RFV_TBemail" runat="server" ErrorMessage="Este campo es requerido" ControlToValidate="TB_email"
                                            ForeColor="Red" Font-Bold="true" ValidationGroup="Registrarse"></asp:RequiredFieldValidator>
                                    </div>

                                    <div class="Form-group2">
                                        <label>Ingresar la Contraseña</label>
                                        <asp:TextBox CssClass="form-control" ID="TB_Password" runat="server" TextMode="Password" MaxLength="50" Width="100%"> </asp:TextBox>
                                        <asp:RequiredFieldValidator ID="RFV_TBPassword" runat="server" ErrorMessage="Este campo es requerido" ControlToValidate="TB_Password"
                                            ForeColor="Red" Font-Bold="true" ValidationGroup="Registrarse"></asp:RequiredFieldValidator>
                                        <br />
                                        <asp:RegularExpressionValidator ID="RegularExpressionValidator1" runat="server" ControlToValidate="TB_Password" ErrorMessage="La contraseña debe tener minimo 8 caracteres y al menos una mayúscula, una minúscula y un número" ForeColor="Red" ValidationExpression="^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)[a-zA-Z\d]{8,}$" />
                                    </div>

                                    <div class="Form-group2">
                                        <label>Ingresar nuevamente la Contraseña</label>
                                        <asp:TextBox CssClass="form-control" ID="TB_Password2" runat="server" TextMode="Password" MaxLength="50" Width="100%"></asp:TextBox>
                                        <asp:RequiredFieldValidator ID="RFV_TBPassword2" runat="server" ErrorMessage="Este campo es requerido" ControlToValidate="TB_Password2"
                                            ForeColor="Red" Font-Bold="true" ValidationGroup="Registrarse"></asp:RequiredFieldValidator>
                                    </div>

                                    <div class="Form-group2">
                                        <br />
                                        <div class="g-recaptcha" data-sitekey="6LfFQLUUAAAAANh0hqzhgSBHplO4oy_jOx37LtuV"></div>
                                    </div>
                                    <div>
                                    </div>
                                    <%--Contenido del Modal--%>
                                </div>

                                <div class="modal-footer">
                                    <asp:CheckBox ID="ck_Terminos" runat="server" />
                                    <p>Al hacer clic en "Registrarse", acepto la <a href="Politicas.aspx">Política de privacidad</a> y <a href="Terminos_y_Condiciones.aspx">las Condiciones </a>Generales de Mokar.</p>
                                    <asp:Button CssClass="btn btn-primary" ID="btn_Salir_LOGIN" runat="server" Text="Salir" />
                                    <asp:Panel runat="server" ID="Panel_Boton_Registrarse">
                                        <asp:Button CssClass="btn btn-primary" ID="btn_Ingresar_REG" runat="server" Text="Registrarse" ValidationGroup="Registrarse" />

                                    </asp:Panel>
                                </div>
                            </div>
