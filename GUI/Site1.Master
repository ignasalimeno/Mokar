                                <asp:RequiredFieldValidator ID="RFV_TBemail1" runat="server" ErrorMessage="Este campo es requerido" ControlToValidate="TB_email1"
                                    ForeColor="Red" Font-Bold="true" ValidationGroup="OlvidoPass"></asp:RequiredFieldValidator>
                            </div>
                            <br />
                            <div class="Form-group">
                                <label>Ingresar nuevamente el mail</label>
                                <asp:TextBox CssClass="form-control" ID="TB_email2" runat="server" TextMode="Email" MaxLength="50" Width="100%"></asp:TextBox>
                                <asp:RequiredFieldValidator ID="RFV_TBemail2" runat="server" ErrorMessage="Este campo es requerido" ControlToValidate="TB_email2"
                                    ForeColor="Red" Font-Bold="true" ValidationGroup="OlvidoPass"></asp:RequiredFieldValidator>
                            </div>
                            <br />
                            <a runat="server" id="A1">Se enviará un correo para validar la identidad</a>
                            <br />
                            <br />
                            <div class="Form-group">
                                <asp:Button CssClass="btn btn-primary mb-2" ID="btn_Enviar" runat="server" Text="Enviar" ValidationGroup="OlvidoPass" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <%-- Fin Modal olvido contraseña --%>


            <!-- Modal -->
            <div class="modal fade" id="Modal_Login" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Login</h5>
